{
  "name": "aldnoah-ssr",
  "version": "0.0.1",
  "description": "A scaffold based on vue and webpack for ssr project",
  "repository": "git@github.com:wenkanglin/aldnoah-ssr.git",
  "author": "WenKang Lin <wenkanglin0910@gmail.com>",
  "license": "MIT",
  "keywords": ["scaffold", "vue", "webpack", "vue-ssr", "aldnoah"],
  "engines": {
    "node": "8.9.4",
    "npm": "5.6.0"
  },
  "scripts": {
    "version:check":
      "node -r ./launcher/utils/error-handler ./launcher/commands/version-check",
    "data:move":
      "node -r ./launcher/utils/error-handler ./launcher/commands/data-move",
    "dll:dev":
      "cross-env NODE_ENV=development node -r ./launcher/utils/error-handler ./launcher/commands/dll-dev",
    "dll:build":
      "cross-env NODE_ENV=production node -r ./launcher/utils/error-handler ./launcher/commands/dll-prod",
    "build:clean":
      "node -r ./launcher/utils/error-handler ./launcher/commands/build-clean",
    "build:client":
      "node -r ./launcher/utils/error-handler ./launcher/commands/build-client",
    "build:server":
      "node -r ./launcher/utils/error-handler ./launcher/commands/build-server",
    "server:dev":
      "cross-env NODE_ENV=development node -r ./launcher/utils/error-handler ./launcher/server/app-dev",
    "server:build":
      "cross-env NODE_ENV=production node -r ./launcher/utils/error-handler ./launcher/server/app-prod",
    "dev": "npm run dll:dev && npm run server:dev",
    "build":
      "npm run build:clean && npm run dll:build && run-p data:move build:client build:server",
    "analyse":
      "npm run build:clean && node -r ./launcher/utils/error-handler ./launcher/commands/analyse",
    "deploy":
      "npm run version:check && npm run build && pm2 start pm2.config.js",
    "deploy:test":
      "npm run version:check && npm run build && pm2 start pm2.config.js --env test"
  },
  "dependencies": {
    "axios": "0.18.0",
    "core-js": "2.5.3",
    "lodash.camelcase": "4.3.0",
    "lodash.kebabcase": "4.1.1",
    "vue": "2.5.16",
    "vue-router": "3.0.1",
    "vue-server-renderer": "2.5.16",
    "vuex": "3.0.1",
    "vuex-router-sync": "5.0.0"
  },
  "devDependencies": {
    "babel-core": "6.26.0",
    "babel-eslint": "8.2.2",
    "babel-loader": "7.1.4",
    "babel-preset-aldnoah": "0.0.1",
    "chalk": "2.3.2",
    "chokidar": "2.0.2",
    "cli-clear": "1.0.4",
    "compression": "1.7.2",
    "copy-webpack-plugin": "4.5.1",
    "cross-env": "5.1.4",
    "css-loader": "0.28.11",
    "cssnano": "3.10.0",
    "del": "3.0.0",
    "eslint": "4.19.1",
    "eslint-config-airbnb-base": "12.1.0",
    "eslint-config-aldnoah": "0.0.1",
    "eslint-import-resolver-webpack": "0.8.4",
    "eslint-loader": "2.0.0",
    "eslint-plugin-import": "2.10.0",
    "eslint-plugin-vue": "4.4.0",
    "express": "4.16.3",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.11",
    "friendly-errors-webpack-plugin": "1.6.1",
    "fs-extra": "5.0.0",
    "globby": "8.0.1",
    "happypack": "4.0.1",
    "http-proxy-middleware": "0.18.0",
    "ip": "1.1.5",
    "lru-cache": "4.1.2",
    "memory-fs": "0.4.1",
    "npm-run-all": "4.1.2",
    "object-hash": "1.3.0",
    "opn": "5.3.0",
    "optimize-css-assets-webpack-plugin": "3.2.0",
    "postcss-loader": "2.1.3",
    "postcss-preset-aldnoah": "0.0.1",
    "pretty-error": "2.1.1",
    "progress": "2.0.0",
    "progress-bar-webpack-plugin": "1.11.0",
    "route-cache": "0.4.4",
    "semver": "5.5.0",
    "serve-favicon": "2.4.5",
    "stylelint": "9.2.0",
    "stylelint-config-aldnoah": "0.0.1",
    "stylelint-config-prettier": "3.2.0",
    "stylelint-config-standard": "18.2.0",
    "sw-precache-webpack-plugin": "0.11.5",
    "url-loader": "1.0.1",
    "vue-loader": "14.2.1",
    "vue-template-compiler": "2.5.16",
    "webpack": "3.11.0",
    "webpack-bundle-analyzer": "2.11.1",
    "webpack-dev-middleware": "2.0.4",
    "webpack-hot-middleware": "2.21.2",
    "webpack-merge": "4.1.2",
    "webpack-node-externals": "1.6.0",
    "webpack-parallel-uglify-plugin": "1.1.0"
  }
}
